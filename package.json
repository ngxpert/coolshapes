{
  "name": "coolshapes",
  "version": "0.0.0",
  "publishConfig": {
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngxpert/coolshapes.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate-shapes": "svg-to-ts-constants",
    "prepare": "husky",
    "commit": "cz",
    "copy:readme": "cpx README.md dist/ngxpert/coolshapes",
